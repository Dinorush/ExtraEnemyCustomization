			},
			"MaterialSets": [
				{
					//Head Part (Kinda works)
					"From": "mtr_striker_bullrush_head",
					"To": "mtr_striker_wave"
				},
				{
					//Body Part
					"From": "mtr_striker_bullrush",
					"To": "mtr_striker_wave"
				}
			]
		},
		{
			"Enabled": true, //Make Every Shooter Oiled, Except Rapid Shooter
			"Target": {
				"Mode": "PersistentID",
				"persistentIDs": [
					26, //Hibernate
					18, //Big
					11 //Wave
				],
				"nameParam": ""
			},
			"MaterialSets": [
				{
					//Hibernate Shooter
					"From": "shooter_hibernate",
					"To": "mtr_striker_bullrush"
				},
				{
					//Big Shooter
					"From": "shooterBig",
					"To": "mtr_striker_bullrush"
				},
				{
					//Wave Shooter
					"From": "shooter_wave",
					"To": "mtr_striker_bullrush"
				}
			]
		},
		{
			"Enabled": true, //Make Birthers Oiled?
			"Target": {
				"Mode": "NameContains",
				"persistentIDs": [],
				"nameParam": "Birther"
			},
			"MaterialSets": [
				{
					"From": "birther",
					"To": "mtr_striker_bullrush"
				}
			]
		}
